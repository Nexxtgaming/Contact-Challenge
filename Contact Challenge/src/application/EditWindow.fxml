<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<DialogPane prefHeight="257.0" prefWidth="240.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <header>
      <Label text="Edit contact" />
   </header>
   <content>
      <GridPane hgap="10.0" vgap="10.0">
         
            <TextField fx:id="firstNameTextArea" GridPane.columnIndex="2" GridPane.columnSpan="2" />
            <Label text="First Name" GridPane.columnSpan="2" />
            <TextField fx:id="lastNameTextArea" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="1" />
            <Label text="Last Name" GridPane.columnSpan="2" GridPane.rowIndex="1" />
            <TextField fx:id="phoneNumberTextArea" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="2" />
            <Label text="Phone Number" GridPane.columnSpan="2" GridPane.rowIndex="2" />
            <TextField fx:id="notesTextArea" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="3" />
            <Label text="Notes" GridPane.columnSpan="2" GridPane.rowIndex="3" />
         
         
         
      </GridPane>
   </content>
</DialogPane>
